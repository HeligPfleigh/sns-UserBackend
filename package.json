{
  "name": "sns-project",
  "version": "0.15.0",
  "private": true,
  "engines": {
    "node": ">=6.5",
    "npm": ">=3.10"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "dependencies": {
    "babel-polyfill": "^6.22.0",
    "bcrypt": "^1.0.2",
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.0",
    "busboy": "^0.2.14",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "cors": "^2.8.3",
    "ejs": "^2.5.7",
    "email-templates": "^2.6.0",
    "express": "^4.14.1",
    "express-graphql": "^0.6.7",
    "express-jwt": "^5.1.0",
    "fastclick": "^1.0.6",
    "firebase": "^3.7.5",
    "firebase-admin": "^4.2.0",
    "font-awesome": "^4.7.0",
    "graphql": "^0.10.5",
    "graphql-tag": "^2.4.2",
    "graphql-tools": "^1.1.0",
    "history": "^4.5.1",
    "jsonwebtoken": "^7.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.0",
    "mongoose": "^4.11.5",
    "mongoose-auto-increment": "^5.0.1",
    "mongoose-timestamp": "^0.6.0",
    "node-fetch": "^1.6.3",
    "nodemailer": "^4.0.1",
    "passport": "^0.3.2",
    "passport-facebook-token": "^3.3.0",
    "passport-local": "^1.0.0",
    "query-string": "^4.3.1",
    "rbac": "^4.0.2",
    "serialize-javascript": "^1.3.0",
    "shortid": "^2.2.8",
    "source-map-support": "^0.4.11",
    "uuid": "^3.0.1",
    "whatwg-fetch": "^2.0.2",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.4.6",
    "winston-syslog": "^1.2.6"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^6.7.2",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-rewire": "^1.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "babel-template": "^6.25.0",
    "babel-types": "^6.25.0",
    "browser-sync": "^2.18.7",
    "chai": "^3.5.0",
    "chokidar": "^1.6.1",
    "cross-env": "^5.0.0",
    "editorconfig-tools": "^0.1.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "extract-text-webpack-plugin": "2.1.0",
    "glob": "^7.1.1",
    "jest": "^20.0.3",
    "lint-staged": "^3.3.0",
    "markdown-it": "^8.2.2",
    "mkdirp": "^0.5.1",
    "mocha": "^3.5.0",
    "pixrem": "^3.0.2",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.1",
    "sinon": "^2.4.1",
    "webpack": "^2.2.1",
    "webpack-bundle-analyzer": "^2.3.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.16.1",
    "write-file-webpack-plugin": "^3.4.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "stage-2"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ],
    "env": {
      "test": {
        "plugins": [
          "rewire"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb-base"
    ],
    "plugins": [],
    "globals": {
      "__DEV__": true
    },
    "env": {
      "browser": true,
      "jest": true
    },
    "rules": {
      "import/extensions": "off",
      "import/no-extraneous-dependencies": "off"
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single",
      "property-no-unknown": [
        true,
        {
          "ignoreProperties": [
            "composes"
          ]
        }
      ],
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ]
    }
  },
  "lint-staged": {
    "*.{cmd,html,json,md,sh,txt,xml,yml}": [
      "editorconfig-tools fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{css,less,scss,sss}": [
      "stylefmt",
      "stylelint",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "!src/**/*.test.{js}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 98,
        "branches": 91,
        "functions": 98,
        "lines": 98
      }
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^mongoose$": "<rootDir>/node_modules/mongoose"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test/test-bundler.js",
    "testRegex": "tests/.*\\.test\\.js$"
  },
  "scripts": {
    "lint:js": "eslint src tools --fix",
    "lint:css": "stylelint \"src/**/*.{css,less,scss,sss}\"",
    "lint:staged": "lint-staged",
    "lint": "yarn run lint:js && yarn run lint:css",
    "test": "cross-env NODE_ENV=test jest --coverage --forceExit --runInBand",
    "testC": "cross-env NODE_ENV=test jest ./src/data/tests/RootDeleteBuildingAnnouncementMutation.test.js --forceExit --runInBand",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "clean": "babel-node tools/run clean",
    "copy": "babel-node tools/run copy",
    "bundle": "babel-node tools/run bundle",
    "build": "babel-node tools/run build",
    "build:stats": "yarn run build -- --release --analyse",
    "deploy": "babel-node tools/run deploy",
    "render": "babel-node tools/run render",
    "serve": "babel-node tools/run runServer",
    "start": "babel-node tools/run start",
    "validate": "npm ls"
  },
  "pre-commit": []
}
